{
  "name": "excel",
  "version": "1.0.0",
  "description": "Simple node excel like application",
  "main": "dist/index.js",
  "repository": "git@github.com:kobaska/excel.git",
  "author": "kogulan.baskaran@gmail.com",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "prestart": "yarn tsc",
    "start": "node dist"
  },
  "dependencies": {
    "papaparse": "^5.3.0",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.3",
    "vm2": "^3.9.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/papaparse": "^5.2.2",
    "jest": "^26.4.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
